	<queryString language="SQL">
		<![CDATA[select cortes.id, cortes.estado, cortes.fecha_corte,  
concat (clientes.primer_nombre, ' ', clientes.segundo_nombre, ' ', clientes.primer_apellido, ' ', clientes.segundo_apellido) as cliente,
contrato.descripcion,
cortes.fecha_corte
from cortes
inner join clientes on cortes.cliente_id = clientes.id
inner join contrato on cortes.contrato_id = contrato.id
where cortes.estado = 'activo']]>
	</queryString>
